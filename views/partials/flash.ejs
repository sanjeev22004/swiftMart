<% if (success && success.length) { %>
  <div class="alert alert-success" role="alert">
    <% success.forEach(message => { %>
      <%= message %><br>
    <% }); %>
  </div>
<% } %>

<% if (error && error.length) { %>
  <div class="alert alert-danger" role="alert">
    <% error.forEach(message => { %>
      <%= message %><br>
    <% }); %>
  </div>
<% } %>
