


<div class="container">
    <% if (success && success.length) { %>
      <div class="alert alert-success" role="alert">
        <%= success %>
      </div>
    <% } %>
</div>

<div class="container">
    <% if (error && error.length) { %>
      <div class="alert alert-danger" role="alert">
        <%= error %>
      </div>
    <% } %>
</div>
  